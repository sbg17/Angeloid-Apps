# vm
echo 50 > /proc/sys/vm/swappiness;
echo 3072 > /proc/sys/vm/min_free_kbytes;
echo 3072 > /proc/sys/vm/dirty_expire_centisecs;
echo 50 > /proc/sys/vm/vfs_cache_pressure;
echo 50 > /proc/sys/vm/swappiness
echo 3072 > /proc/sys/vm/min_free_kbytes
echo 3072 > /proc/sys/vm/dirty_expire_centisecs
echo 50 > /proc/sys/vm/vfs_cache_pressure